@model IEnumerable<JinxiaocunApp.Models.BStore>

@{
    ViewBag.Title = "Index";
}

<div class="panel panel-info">

<p>
    @Ajax.ActionLink("新建", "Create", new { id = ViewBag.SuperRef }, new AjaxOptions { UpdateTargetId = "formDiv", HttpMethod = "Get", InsertionMode = InsertionMode.Replace, OnSuccess = "showW('存货仓库-新建')" }, new { @class="btn"})
</p>
<table class="table-striped table-hover table-bordered" width="100%">
        <tr class="alert-info">
        <th>
            @Html.DisplayNameFor(model => model.NumberStore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FullNameStore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SmallNameStore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PinyinStore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RemarkStore)
        </th>
        <th></th>
    </tr>
    <tbody id="@ViewBag.PageDiv">
            @Html.Action("Pages", "BStore")
        </tbody>
    </table>
    @Html.Action("PaginationPartial", "MyTart", new { id = ViewBag.Pagination, TotalPage = ViewBag.TotalPage, tbodyPages = ViewBag.PageDiv, ControllerName = "BStore" })

</div>