@{
    ViewBag.Title = "_layoutT";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@section footerT{
    <section>
        @Html.Partial("_FooterMaster")
    </section>
}
<div class="row">
    <div class="col-md-12 col-lg-12">
        @RenderSection("InheadSec", required: false)
    </div>
</div>
<div class="row" style="padding:0 0 0 15px;">
    <div class="col-md-3 col-lg-3">
        <div class="row">
            @Html.Action("LoginInner", "Account")
        </div>
        <div class="row">
            @Html.Partial("_LinkType")
        </div>
    </div>
    <div class="col-md-9 col-lg-9">
        @RenderBody()
        @RenderSection("scripts", required: false)
    </div>
</div>
